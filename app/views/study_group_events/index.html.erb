<p id="notice"><%= notice %></p>

<h1>Study Group Events</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_group_events.each do |study_group_event| %>
      <tr>
        <td><%= study_group_event.date %></td>
        <td><%= link_to 'Show', study_group_event %></td>
        <td><%= link_to 'Edit', edit_study_group_event_path(study_group_event) %></td>
        <td><%= link_to 'Destroy', study_group_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Study Group Event', new_study_group_event_path %>
