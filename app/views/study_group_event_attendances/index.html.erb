<p id="notice"><%= notice %></p>

<h1>Study Group Event Attendances</h1>

<table>
  <thead>
    <tr>
      <th>Study group attendance</th>
      <th>Study group event</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_group_event_attendances.each do |study_group_event_attendance| %>
      <tr>
        <td><%= study_group_event_attendance.study_group_attendance_id %></td>
        <td><%= study_group_event_attendance.study_group_event_id %></td>
        <td><%= link_to 'Show', study_group_event_attendance %></td>
        <td><%= link_to 'Edit', edit_study_group_event_attendance_path(study_group_event_attendance) %></td>
        <td><%= link_to 'Destroy', study_group_event_attendance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

